{
  "name": "typed_i18n",
  "version": "0.4.6",
  "description": "",
  "bin": "index.js",
  "scripts": {
    "build:native": "jbuilder build src/typed_i18n.exe",
    "build:js": "jbuilder build src/typed_i18n.bc.js",
    "start": "node _build/default/src/typed_i18n.bc.js",
    "pre:publish": "mv _build/default/src/typed_i18n.bc.js index.js && npm version patch",
    "deploy": "npm publish --access public"
  },
  "esy": {
    "build": [
      [
        "jbuilder",
        "build",
        "src/typed_i18n.bc.js"
      ]
    ],
    "install": [
      "esy-installer"
    ],
    "buildsInSource": "_build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kogai/typed_i18n.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kogai/typed_i18n/issues"
  },
  "homepage": "https://github.com/kogai/typed_i18n#readme",
  "dependencies": {
    "@esy-ocaml/esy-installer": "^0.0.0",
    "@opam/cmdliner": "^1.0.2",
    "@opam/easy-format": "^1.3.1",
    "@opam/jbuilder": "^1.0.0-beta17",
    "@opam/js_of_ocaml": "^3.0.2",
    "@opam/js_of_ocaml-lwt": "^3.0.2",
    "@opam/merlin": "^3.0.5",
    "@opam/ppx_blob": "^0.4.0",
    "@opam/yojson": "^1.4.1",
    "ocaml": "^4.6.1"
  }
}
